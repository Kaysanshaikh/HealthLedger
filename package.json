{
  "$schema": "https://json.schemastore.org/package",
  "name": "healthledger-ipfs",
  "version": "1.0.0",
  "description": "HealthLedger on Polygon with IPFS integration",
  "main": "index.js",
  "scripts": {
    "clean": "hardhat clean",
    "compile": "hardhat compile",
    "test": "hardhat test",
    "node": "hardhat node",
    "deploy:amoy": "hardhat run scripts/deploy.js --network polygonAmoy",
    "deploy:local": "hardhat run scripts/deploy.js --network localhost",
    "add:record": "hardhat run scripts/addRecord.js --network polygonAmoy",
    "get:record": "hardhat run scripts/getRecord.js --network polygonAmoy",
    "grant:access": "hardhat run scripts/grantAccess.js --network polygonAmoy",
    "add:doctor": "hardhat run scripts/addDoctor.js --network polygonAmoy",
    "build": "cd frontend && npm install && npm run build",
    "start": "node server.js",
    "start:api": "node server.js",
    "dev:api": "nodemon server.js",
    "db:init": "node database/init.js",
    "db:test": "node database/testConnection.js",
    "db:test-full": "node database/testDatabase.js",
    "db:view": "node database/viewData.js",
    "db:cleanup": "node database/cleanupTest.js",
    "db:sync": "node database/syncBlockchainToDb.js",
    "test:pinata": "node scripts/testPinata.js",
    "sync:blockchain": "node scripts/syncBlockchain.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "hardhat": "^2.22.0",
    "nodemon": "^3.1.0"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.0.0",
    "axios": "^1.7.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.0",
    "ethers": "^6.10.0",
    "express": "^4.19.2",
    "form-data": "^4.0.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.16.3",
    "uuid": "^9.0.1",
    "web3.storage": "^4.5.5"
  }
}
